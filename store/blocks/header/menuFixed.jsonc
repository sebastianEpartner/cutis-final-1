{
    "flex-layout.row#sticky-MenuFixed": {
        "children": [
            "flex-layout.col#cuenta",
            "flex-layout.col#tiendas",
            "flex-layout.col#kits",
            "flex-layout.col#ofertas",
            "flex-layout.col#wp"
        ],
        "props": {
          "blockClass": "fixed-menu"
        }
    },
    // cuenta
    "flex-layout.col#cuenta": {
        "props": {
          "blockClass":"menu-item-fixed"
        },
        "children":[
            "image#cuenta",
            "rich-text#cuenta-fixed"
        ]
    },
    "image#cuenta": {
        "props": {
          "src": "https://cutis.vteximg.com.br/arquivos/user-mobile.png",
          "maxHeight": 25,
          "blockClass":"item-menu-fixed"
        }
    },
    "rich-text#cuenta-fixed": {
        "props": {
          "text": "[Cuenta](/account)",
          "blockClass": ["item-menu-fixed"]
        }
    },
    // tiendas
    "flex-layout.col#tiendas": {
        "props": {
          "blockClass":"menu-item-fixed"
        },
        "children":[
            "image#tiendas",
            "rich-text#tiendas-fixed"
        ]
    },
    "image#tiendas": {
        "props": {
          "src": "https://cutis.vteximg.com.br/arquivos/ubicacion-mobile.png",
          "link":{
            "url":"/account"
          },
          "maxHeight": 25,
          "blockClass":"item-menu-fixed"
        }
    },
    "rich-text#tiendas-fixed": {
        "props": {
          "text": "[Tiendas](/account)",
          "blockClass": ["item-menu-fixed"]
        }
    },
    // kits
