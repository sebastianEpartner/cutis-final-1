        "props": {
          "src": "https://cutis.vteximg.com.br/arquivos/kits-mobile.png",
          "link":{
            "url":"/137?map=productClusterIds&order="
          },
          "maxHeight": 25,
          "blockClass":"item-menu-fixed"
        }
    },
    "rich-text#kits-fixed": {
        "props": {
          "text": "[Kits](/143?map=productClusterIds&order=)",
          "blockClass": ["item-menu-fixed"]
        }
