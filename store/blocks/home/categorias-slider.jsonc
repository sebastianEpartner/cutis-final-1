{
    "slider-layout#principalSliderHomeCate": {
        "props": {
            "itemsPerPage": {
                "desktop": 1,
                "tablet": 1,
                "phone": 1
            },
            // "autoplay":{
            //     "timeout": 3000,
            //     "stopOnHover": true
            // },
            "infinite": true,
            "blockClass": "carousel",
            "showPaginationDots":"mobileOnly",
            "showNavigationArrows": "desktopOnly"
        },
        "children": [
            "flex-layout.row#categoriaSlide1",
            "flex-layout.row#categoriaSlide2"
        ]
    },
    // Primer SLIDE
    "flex-layout.row#categoriaSlide1": {
        "children": [
          "flex-layout.col#responsive-slider",
          "flex-layout.col#categoriaSlide1-2"
        ],
        "props": {
          "blockClass": "slideCategory"
        }
    },
    // contenedor de los responsive
    "flex-layout.col#responsive-slider": {
      "children": [
        "responsive-layout.mobile#categoriaSlider1",
        "responsive-layout.desktop#categoriaSlider1"
      ],
      "props": {
        "blockClass": "responsiveSlider"
      }
    },
    // estructura mobile
    "responsive-layout.mobile#categoriaSlider1": {
      "children": [
        "flex-layout.row#categoriaSlide1-1-m",
        "flex-layout.col#categoriaSlide1-1-m"
      ]
    },
    "flex-layout.row#categoriaSlide1-1-m": {
      "children": [
          "rich-text#slideCate-title",
          "rich-text#slideCate-text"
      ],
      "props": {
        "blockClass": "slideColumn1"
      }
    },
    "flex-layout.col#categoriaSlide1-1-m": {
      "children": [
          "flex-layout.row#categoriaSlide1-img-COLUMN1"
      ],
      "props": {
        "blockClass": "slideColumn1"
      }
    },
    // estructura para desk
    "responsive-layout.desktop#categoriaSlider1": {
      "children": [
        "flex-layout.col#categoriaSlide1-1"
      ]
    },
    "flex-layout.col#categoriaSlide1-1": {
        "children": [
            "rich-text#slideCate-title",
            "rich-text#slideCate-text",
            "flex-layout.row#categoriaSlide1-img-COLUMN1"
        ],
        "props": {
          "blockClass": "slideColumn1"
        }
    },
    // columna 1
    "rich-text#slideCate-title": {
        "props": {
          "text": "Conoce nuestras categorías",
          "blockClass": "tituloSlideCategory"
        }
    },
    "rich-text#slideCate-text": {
        "props": {
          "text": "Tenemos los mejores productos para el cuidado y belleza de tu piel",
          "blockClass": "contenidoSlideCategory"
        }
    },
    // Imagenes columna 1
    "flex-layout.row#categoriaSlide1-img-COLUMN1": {
        "children": [
          "flex-layout.col#imagenCategoria1",
          "flex-layout.col#imagenCategoria2"
        ],
        "props": {
          "blockClass": "imagesColumn1"
        }
    },
    // imagen uno de categorias
    "flex-layout.col#imagenCategoria1": {
      "children": [
        "image#slide1category-1",
        "rich-text#tituloSliderCategorias",
        "image#slide1category-1-hover"
      ],
      "props": {
        "blockClass": "imagenCategoriaColumn"
      }
    },
    "rich-text#tituloSliderCategorias": {
      "props": {
        "text": "[Cuidado facial](/)",
        "blockClass": ["categoriasTitulo"]
      }
    },
    "image#slide1category-1": {
        "props": {
          "src": "https://cutis.vtexassets.com/arquivos/categoria-facil.png",
          "blockClass": ["slideCategoryColumn1","bannerSlides"],
          "link":{
            "url":"/"
          }
        }
  },
  "image#slide1category-1-hover": {
      "props": {
        "src": "https://cutis.vtexassets.com/arquivos/facial-icono.png",
        "blockClass": ["hoverImage"],
        "link":{
          "url":"/"
        }
      }
  },
  // imagen dos de categorias
  "flex-layout.col#imagenCategoria2": {
    "children": [
      "image#slide2category-2",
      "rich-text#tituloSliderCategorias-2",
      "image#slide1category-2-hover"
    ],
    "props": {
      "blockClass": "imagenCategoriaColumn"
    }
  },
  "rich-text#tituloSliderCategorias-2": {
    "props": {
      "text": "[Cuidado capilar](/)",
      "blockClass": ["categoriasTitulo"]
    }
  },
  "image#slide2category-2": {
      "props": {
        "src": "https://cutis.vtexassets.com/arquivos/categoria-capilar.png",
        "blockClass": ["slideCategoryColumn1","bannerSlides"],
        "link":{
          "url":"/"
        }
      }
  },
  "image#slide1category-2-hover": {
    "props": {
      "src": "https://cutis.vtexassets.com/arquivos/capilar-icono.png",
      "blockClass": ["hoverImage"],
      "link":{
        "url":"/"
      }
    }
  },
  // Columna 2
    "flex-layout.col#categoriaSlide1-2": {
        "children": [
          "flex-layout.col#imagen-3"
        ],
        "props": {
          "blockClass": "slideColumn2"
        }
    },
