      "props": {
        "src": "https://cutis.vtexassets.com/arquivos/principalCategoria.png",
        "blockClass": ["slideCategoryColumn2","bannerSlides"],
        "link":{
          "url":"/"
        }
      }
  },
  "image#slide1category-3-hover": {
    "props": {
      "src": "https://cutis.vtexassets.com/arquivos/corporal-icono.png",
      "blockClass": ["hoverImage","hoverColumn2"],
      "link":{
        "url":"/"
      }
    }
  },
  // SEGUNDA SLIDE
  "flex-layout.row#categoriaSlide2": {
    "children": [
      "flex-layout.col#responsive-slider2",
      "flex-layout.col#categoriaSlide2-2"
    ],
    "props": {
      "blockClass": "slideCategory"
    }
  },
